    <style name="Button5">
        <item name="cornerSize">50%</item>
    </style>

    <style name="PrimaryButton">
        <item name="cornerSize">50%</item>
        <item name="android:textColor">@color/white_100</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_width">311dp</item>
        <item name="backgroundColor">@color/very_dark_blue_100</item>
    </style>

    <style name="PrimaryButton_dynamic">
        <item name="cornerSize">50%</item>
